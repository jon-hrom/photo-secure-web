{
  "tests": [
    {
      "name": "OPTIONS request for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200
    },
    {
      "name": "Missing parameters returns error",
      "method": "GET",
      "path": "/?userId=1",
      "expectedStatus": 400,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Non-existent file returns 404",
      "method": "GET",
      "path": "/?key=incoming/999/nonexistent.jpg&userId=999",
      "expectedStatus": 404,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}