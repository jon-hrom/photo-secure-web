{
  "tests": [
    {
      "name": "OPTIONS request for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200
    },
    {
      "name": "List trash without auth",
      "method": "GET",
      "path": "/?action=list",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "List trash with auth",
      "method": "GET",
      "path": "/?action=list",
      "headers": {
        "X-User-Id": "1"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "trashed_folders": []
      },
      "bodyMatcher": "partial"
    }
  ]
}
