{
  "tests": [
    {
      "name": "OPTIONS for CORS preflight",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedHeaders": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "name": "List plans without admin key",
      "method": "GET",
      "path": "/?action=list-plans",
      "expectedStatus": 403,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "List plans with admin key",
      "method": "GET",
      "path": "/?action=list-plans",
      "headers": {
        "X-Admin-Key": "admin123"
      },
      "expectedStatus": 200
    }
  ]
}