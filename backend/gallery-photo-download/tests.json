{
  "tests": [
    {
      "name": "Missing s3_key",
      "method": "GET",
      "expectedStatus": 400
    },
    {
      "name": "Valid s3_key returns error for non-existent file",
      "method": "GET",
      "path": "/?s3_key=test/nonexistent.jpg",
      "expectedStatus": 500,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "HEAD request returns headers",
      "method": "HEAD",
      "path": "/?s3_key=test/file.jpg",
      "expectedStatus": 500
    }
  ]
}